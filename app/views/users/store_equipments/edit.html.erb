<h1>Users::StoreEquipments#edit</h1>
<p>Find me in app/views/users/store_equipments/edit.html.erb</p>


<div>
	<%= form_with(model: @store_equipment, url: users_store_equipment_path(@store_equipment), local: true, method: :patch) do |f| %>
		<%= f.label :genre_name, '設備ジャンル' %>
		<%= f.text_field :genre_name %>
		<%= f.label :display_status?, '有効' %>
		<%= f.check_box :display_status?, {}, true %>

		<%= f.label :display_status?, '無効' %>
		<%= f.check_box :display_status?, {}, false %>
		<%= f.submit "変更を保存する" %>
	<% end %>
</div>
